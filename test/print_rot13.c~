#include "main.h"
void rot13(char *str)
{
	char *ptr;
	*ptr = str;
	while (*ptr !='\0')
	{
		if ((*ptr >= 'A' && *ptr <= 'M') || (*ptr >= 'a' && *prt <= 'm'))
			*ptr += 13;
		else if ((*ptr >= 'N' && *ptr <= 'Z') || (*ptr >= 'n' && *ptr <= 'z'))
			*ptr -= 13;
		*ptr++
	}
	printf("%s/n", str);
}
int main()
{
	rot13("yoo how are you doing");
	return 0;
}
