#include <stdarg.h>
#include "main.h"
int _printf(const char *format, ...)
{
	char *countchar;
	int i;
	char *s;
	va_list arg;
	va_start(arg, format);
	for (*countchar = *format; *countchar != '\0'; countchar++)
	{
		while (*countchar != '%')
		{
			_putchar(*countchar);
			countchar++;
		}
		countchar++;
		switch(*countchar)
		{
		case 'c': i = va_arg(arg, int);
			_putchar(i);
			break;
		case 's': s = va_arg(arg, char *);
			_printstr(s);
			break;
		case '%': _putchar('%');
			break;
		}
	}
	va_end(arg);
	return (0);
}
